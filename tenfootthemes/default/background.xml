<?xml version="1.0" encoding="UTF-8" ?>
<torctheme>
    <window name="basewindow" template="true" position="0,0,1920,1080">
        <effect alpha="0"/>
        <animation name="basewindow_Activated">
            <element duration="250" property="alpha" endvalue="1" easingcurve="outquad"/>
        </animation>
        <connect signal="Finished" sender="basewindow_Deactivated" slot="Disable"/>
        <animation name="basewindow_Deactivated">
            <element duration="250" property="alpha" endvalue="0" easingcurve="outquad"/>
        </animation>
    </window>
    <window name="templates" template="true" position="0,0,1,1">
        <state disabled="1"/>
        <button name="templates_button" position="0,0,300,100" template="true">
            <effect alpha="0.5" centre="middle"/>
            <connect signal="Selected"   slot="Start" receiver="templates_button_glow_show"/>
            <connect signal="Deselected" slot="Start" receiver="templates_button_glow_hide"/>
            <connect signal="Deselected" slot="Stop"  receiver="templates_button_glow_show"/>
            <animation name="templates_button_Selected">
                <element property="zoom"  endvalue="1.1" easingcurve="OutQuad" duration="250"/>
                <element property="alpha" endvalue="1.0" easingcurve="OutQuad" duration="250"/>
            </animation>
            <animation name="templates_button_Deselected">
                <element property="alpha" endvalue="0.5" easingcurve="OutQuad" duration="250"/>
                <element property="zoom"  endvalue="1.0" easingcurve="OutQuad" duration="250"/>
            </animation>
            <shape name="templates_button_glow" position="-15,-15,330,130">
                <effect alpha="0.0"/>
                <path>
                    <pen color="FFA000FF" width="15" penstyle="SolidLine"/>
                    <addroundedrect rect="17,17,294,94" xradius="11" yradius="11"/>
                    <fixedblur radius="5"/>
                </path>
                <animation name="templates_button_glow_show">
                    <group type="sequential" loopcount="-1">
                        <element property="alpha" endvalue="0.8" easingcurve="InCubic" duration="500"/>
                        <element property="alpha" endvalue="0.5" easingcurve="OutCubic" duration="500"/>
                    </group>
                </animation>
                <animation name="templates_button_glow_hide">
                    <element property="alpha" endvalue="0" easingcurve="OutCubic" duration="100"/>
                </animation>
            </shape>
            <shape name="templates_button_bg" position="0,0,300,100">
                <path>
                    <pen color="FFA000FF" width="3" penstyle="SolidLine"/>
                    <brush color="FFFFFFFF" brushstyle="SolidPattern"/>
                    <addroundedrect rect="2,2,294,94" xradius="11" yradius="11"/>
                </path>
            </shape>
            <text name="templates_button_text" position="15,15,270,70">
                <font name="main" alignment="center"/>
            </text>
        </button>
        <group name="templates_horizontalgroup" position="0,700,1920,100" template="1">
            <layout type="horizontal" selectrange="100,0,1720,100"/>
            <scroll speed="1000" easingcurve="outquad"/>
        </group>
    </window>
    <window name="gridmenu" from="basewindow">
        <group name="gridmenu_menu" position="200,0,600,800">
            <effect><effect hreflection="800" alpha="0.3"/></effect>
            <clip rect="-100,0,1000,800"/>
            <layout type="grid" fixedwidth="300" fixedheight="100" selectrange="0,0,600,800"/>
            <scroll speed="1000" easingcurve="linear"/>
            <button name="gridmenu_menu_1" from="templates_button">
                <message>Close</message>
                <connect signal="Released" slot="SetAsRootWidget" receiver="topmenu"/>
            </button>
            <button name="gridmenu_menu_2" from="templates_button">
